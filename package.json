{
  "name": "ipam-dashboard",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "start": "next start",
    "dev:dev": "npm run papak:init && next dev",
    "dev": "next dev",
    "build": "npm run papak:init && next build",
    "cli:dev": "npm run papak:init && plop",
    "cli": "plop",
    "papak:clone": "rm -rf papak && rm -rf ./node_modules/papak && git -c http.sslVerify=false -c advice.detachedHead=false clone --branch main  https://gitlab.com/rastegar_m/papak.git ./node_modules/papak && rm -rf ./node_modules/papak/.git",
    "papak:init": "rm -rf ./src/papak && rm -rf ./node_modules/papak  && npm run papak:clone && cp -r ./node_modules/src/papak. ./src/papak/",
    "papak:update": "npm update papak",
    "mock": "json-server --watch -p 8000 db.json"
  },
  "dependencies": {
    "@ant-design/charts": "2.0.3",
    "@ant-design/nextjs-registry": "^1.0.0",
    "@tabler/icons-react": "^3.12.0",
    "ahooks": "^3.7.11",
    "antd": "^5.14.2",
    "axios": "^1.6.7",
    "axios-retry": "^4.1.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "jsonwebtoken": "^9.0.2",
    "next": "14.1.0",
    "next-intl": "^3.9.1",
    "plop": "^4.0.1",
    "qs": "^6.12.1",
    "react": "^18",
    "react-dom": "^18",
    "universal-cookie": "^7.1.0"
  },
  "devDependencies": {
    "@tanstack/query-core": "^5.24.8",
    "@tanstack/react-query": "^5.24.8",
    "@tanstack/react-query-devtools": "^5.24.8",
    "@tanstack/react-query-next-experimental": "^5.24.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "antd-jalali": "^2.0.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
